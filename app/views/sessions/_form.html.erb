<%= form_with(model: session) do |form| %>
  <div>
    <%= form.label :user_name, "User Name" %>
    <%= form.text_field :user_name %>
  </div>
  <div>
    <%= form.label :password, "Password" %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Log In" %>
  </div>
<% end %>