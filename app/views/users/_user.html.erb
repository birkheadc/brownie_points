<div id="<%= dom_id user %>">
  <p>
    <strong>Group:</strong>
    <%= user.group_id %>
  </p>

  <p>
    <strong>User name:</strong>
    <%= user.user_name %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>Nickname:</strong>
    <%= user.nickname %>
  </p>

  <p>
    <strong>Points:</strong>
    <%= user.points %>
  </p>

</div>
