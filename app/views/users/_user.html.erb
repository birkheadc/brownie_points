<div id="<%= dom_id user %>">
  <p>
    <strong>User name:</strong>
    <%= user.user_name %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>Nick name:</strong>
    <%= user.nick_name %>
  </p>

  <p>
    <strong>Points:</strong>
    <%= user.points %>
  </p>

  <p>
    <strong>Partner:</strong>
    <%= user.partner %>
  </p>

</div>
